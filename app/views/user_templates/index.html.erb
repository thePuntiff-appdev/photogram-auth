<div class="container">
  <div class="row">
    <div class="col-md-12">

    </div>
  </div>


  <div class="row">
    <div class="col-md-8 offset-md-2">
      <div class="card">
        <div class="card-header">
          <h3 class="card-title">
            All Users
          </h3>
        </div>

        <ul class="list-group">
          <% @users.each do |user| %>
            <li class="list-group-item">
              <a href="/users/<%=user.id%>">
                <%=user.username%>
              </a>
            </li>
          <% end %>
        </ul>
      </div>
    </div>
  </div>
</div>
